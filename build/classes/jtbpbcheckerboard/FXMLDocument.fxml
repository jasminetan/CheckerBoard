<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jtbpbcheckerboard.FXMLDocumentController">
   <children>
      <MenuBar fx:id="menuBar" prefHeight="15.0" prefWidth="320.0">
        <menus>
          <Menu mnemonicParsing="false" text="Grid">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handle16" text="16 x 16" />
                  <MenuItem mnemonicParsing="false" onAction="#handle10" text="10 x 10" />
                  <MenuItem mnemonicParsing="false" onAction="#handle8" text="8 x 8" />
                  <MenuItem mnemonicParsing="false" onAction="#handle3" text="3 x 3" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Colors">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleDefault" text="Default" />
                  <MenuItem mnemonicParsing="false" onAction="#handleBlue" text="Blue" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane id="AnchorPane" fx:id="board" prefHeight="200" prefWidth="320" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
